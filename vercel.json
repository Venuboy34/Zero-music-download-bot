{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
